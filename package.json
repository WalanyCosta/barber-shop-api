{
  "name": "barber-shop-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "union:test": "DESTY_PATH='schema.test.prisma' tsx prisma-file-union.ts",
    "union:prod": "DESTY_PATH='schema.prisma' tsx prisma-file-union.ts",
    "migrate:dev": "npm run union:test && prisma migrate dev --schema='./prisma/schema.test.prisma'",
    "migrate:prod": "npm run union:test && prisma migrate deploy --schema='./prisma/schema.prisma'",
    "build": "rimraf dist && tsc",
    "start": "npm run migrate:prod && node dist/main/server.js",
    "dev": "tsx watch src/main/server.ts",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "prepared": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prisma": "^5.7.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
},
"dependencies": {
    "@prisma/client": "^5.7.0",
    "tsx": "^4.7.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": "18.x"
  }
}
